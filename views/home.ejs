       <script src="/auth/startup.js"></script>
       
     <script>
     
     $(document).ready(function(){
     $( "#info" ).dialog({
               autoOpen: false, 
               height: 400,
               width:1200 
            });
      $( "#info1" ).dialog({
               autoOpen: false, 
               height: 400,
               width:800 
            });  
      
    
     	$("#courses").click(function(){
     		
